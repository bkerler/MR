<?xml version='1.0' encoding='utf-8' standalone='yes' ?>
<Items>
<Timeline path="Screenshot*.png" type="raw">
        <python run="android_thumbnail.py">
            <type text="Screenshots" category="Media"/>
            <filename>
                <field addfields="{%2}"/>
            </filename>
            <package/>
            <timestamp field="{%0}" int="/1000"/>
            <duration/>
            <desc/>
            <contact/>
            <contact_alias/>
            <message/>
            <image>
                <field addfields="{%1}"/>
            </image>
        </python>
</Timeline>

</Items>